function combineAddress()
{var frmObj=document.frmSubmit;var strName=frmObj.buyercontactname.value;var strAddr1=frmObj.buyeraddress1.value;var strAddr2=typeof(frmObj.buyeraddress2)!="undefined"?frmObj.buyeraddress2.value:null;var strCity=frmObj.buyercity.value;var strStateProvince=typeof(frmObj.buyerstateprovince)!="undefined"?frmObj.buyerstateprovince.value:null;var strZip=frmObj.buyerzip.value;var strCountry=frmObj.buyercountry.options[frmObj.buyercountry.selectedIndex].text;var combinedAddr='';if(strName)
combinedAddr+=strName+'\n';if(strAddr1)
combinedAddr+=strAddr1+'\n';if(strAddr2)
combinedAddr+=strAddr2+'\n';if(strCity&&strStateProvince&&strZip)
combinedAddr+=strCity+', '+strStateProvince+' '+strZip+'\n';else if(strCity&&strStateProvince)
combinedAddr+=strCity+', '+strStateProvince+'\n';else if(strCity&&strZip)
combinedAddr+=strCity+', '+strZip+'\n';else if(strStateProvince&&strZip)
combinedAddr+=strStateProvince+' '+strZip+'\n';else if(strCity)
combinedAddr+=strCity;else if(strStateProvince)
combinedAddr+=strStateProvince;else if(strZip)
combinedAdd+=strZip;if(strCountry)
combinedAddr+=strCountry+'\n';frmObj.QuickAddress.value=combinedAddr;frmObj.QuickAddress.focus();frmObj.QuickAddress.select();}
function parseAddress(){var size;var found=false,gotName=false,gotStreet1=false,gotStreet2=false,gotCity=false,gotState=false;var gotZip=false,gotCountry=false,setName=false,showAlert=false;var iCityStateline=-1;var object=document.frmSubmit;if(object.QuickAddress.value.indexOf("Copy and paste")>-1){return;}
var BuyerArray=new Array();var tr=0;var val=object.QuickAddress.value;val=val.replace(/[\n\r]*/,'');for(var i=0;i<val.length;i++)
{if(isNewlineChar(val.charCodeAt(i)))
{tr++;if((i<val.length-1)&&isNewlineChar(val.charCodeAt(i+1)))
i++;}
else
{if(!BuyerArray[tr])
BuyerArray[tr]="";BuyerArray[tr]+=val.charAt(i);}}
var oldstatevalue=object.buyerstateprovince.value;var skipline=false;var origLine="";for(var j=0;j<BuyerArray.length;j++){origLine=BuyerArray[j];BuyerArray[j]=BuyerArray[j].replace(/\+/g,' ');BuyerArray[j]=BuyerArray[j].replace(/[\t\n\r\f]/g,'');skipline=false;var checkalpha=BuyerArray[j].match(/\W/);if(checkalpha==BuyerArray[j].substring(0,1)){skipline=true;var checkchar=BuyerArray[j].match(/[\w|\d]/);if(checkchar){skipline=false;}}
if(!skipline){var linenospaces=BuyerArray[j].replace(/\+/,'');if(linenospaces.length>=1&&isNaN(linenospaces.substring(0,1))&&gotName==false){object.buyercontactname.value=BuyerArray[j];gotName=true;setName=true;}
else if(BuyerArray[j].length>=1&&gotStreet1==false&&BuyerArray[j].indexOf(",")<0){if(iCityStateline<0||j<iCityStateline){object.buyeraddress1.value=BuyerArray[j];gotStreet1=true;gotName=true;}}
else if(BuyerArray[j].length>=1&&gotName&&!gotCity){if(BuyerArray[j].indexOf(",")>0){iCityStateline=j;var CityState=BuyerArray[j].split(",");var oldBuyerCity=object.buyercity.value;object.buyercity.value=CityState[0];gotCity=true;State=CityState[1].match(/(\w+\s)([\s|\w]*)/);if(State==null){showAlert=true;object.buyercity.value=oldBuyerCity;}
