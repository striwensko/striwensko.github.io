function parseValid(num)
{var parse_num='';var newnum='0';var num_length=num.length;for(var i=0;i<num_length;)
{parse_num=num.substr(i,1);if((!(isNaN(parseInt(parse_num))))||(parse_num==',')||(parse_num=='.')||(parse_num=='-'))
{i++;}
else
{if(i>0)
{newnum=num.substr(0,i)
num=num.substring(i+1);num=newnum+num;num_length=num.length;}
else
{num=num.substring(i+1);num_length--;}}}
return num;}
function parseCountry(num)
{var parse_num='';num_length=num.length;var uscnt=0;var othercnt=0;var cnt=1;for(var i=0;i<num_length;)
{parse_num=num.substr(i,1);if(parse_num==GroupingSymbol)
{if(i>0)
{newnum=num.substr(0,(num.indexOf(parse_num)))
num=num.substring(i+1);num=newnum+num;num_length=num.length;}
else
{num=num.substring(i+1);num_length--;}}
else if(parse_num==ListedDecimal)
{if(num_length==1)
{num=num.substring(i+1);num_length--;}
else
{uscnt++;if(uscnt>1)
{newnum=num.substr(0,i)
num=num.substring(i+1);num=newnum+num;num_length=num.length;}
else
{i++;}}}
else
{i++;}}
return num;}

