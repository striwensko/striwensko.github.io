else{var clr1="#C7D1E8";var clr2="#000000";}
out+='<table width="200" border="0" cellpadding="0" cellspacing="0">\n';out+=' <tr>\n';out+='  <td valign="center">\n';if(siteid=="0"||siteid=="1"||siteid=="2"||siteid=="15"||siteid=="100"||siteid=="212"||siteid=="216"||siteid=="223")
out+='   <input type="button" class="buttonsm" onClick="parseAddress()" value="'+addbutton+'" /><br>\n';out+='  <input type="button" class="buttonsm" onClick="combineAddress()" value="'+combineAddressButtonTxt+'" />\n';out+='  </td>\n';out+='  <td>\n';if(!ds2UI)
