else{object.buyeraddress2.value=BuyerArray[j];gotStreet2=true;}}
else if(BuyerArray[j].length>=1&&gotName&&gotCity&&gotState&&!gotStreet2&&!gotZip){var zippattern=BuyerArray[j].match(/\d+-*\d+/);if(!zippattern||zippattern.length==0||BuyerArray[j]!=zippattern){showAlert=true;}
else{object.buyerzip.value=BuyerArray[j];gotZip=true;iCityStateline=j;}}}}
if(iCityStateline==-1&&BuyerArray.length>=6)
{gotStreet2=true;object.buyercontactname.value=BuyerArray[0];object.buyeraddress1.value=BuyerArray[1];object.buyeraddress2.value=BuyerArray[2];object.buyercity.value=BuyerArray[3];object.buyerstateprovince.value=BuyerArray[4];object.buyerzip.value=BuyerArray[5];}
if(BuyerArray.length>0&&object.buyercountry.length>0){if(BuyerArray[iCityStateline+1]){var country=BuyerArray[iCityStateline+1];var buyercountryLen=object.buyercountry.length;var i;if(country.toLowerCase()=="usa"){country="United States";}
for(i=0;i<buyercountryLen;i++)
{if(country.toLowerCase()==object.buyercountry[i].text.toLowerCase())
{object.buyercountry.selectedIndex=i;gotCountry=true;}}
if(gotCountry==false){showAlert=true;}}}
if(gotStreet2==false&&typeof(object.buyeraddress2)!="undefined"){object.buyeraddress2.value="";}
if(setName){if(iCityStateline>3){showAlert=true;}}else{if(iCityStateline>2){showAlert=true;}}
if(setName&&gotStreet1&&gotZip){if(!gotStreet2&&!gotCountry&&BuyerArray.length>4){showAlert=true;}
if(((gotStreet2&&gotCountry)||(gotStreet2&&!gotCountry))&&BuyerArray.length>5)
showAlert=true;}
version=parseInt(navigator.appVersion);if(navigator.appName=="Microsoft Internet Explorer"||navigator.appName=="MSIE"&&version>=4){if(setName!=true||gotStreet1!=true||gotCity!=true||gotState!=true||gotZip!=true||iCityStateline<0||showAlert){var str,c;c=ebay.oDocument.getConfig('quickfill');if(c)
str=c.sQuickFillAlert;else
str="Sorry, we were unable to fill all of the address fields you entered.\n"+"Please format your input like the example below or enter it manually.\nMake sure there is a comma between city and state.\n"+"              Example:\n"+"                              John Doe\n"+"                              2145 Hamilton Ave\n"+"                              San Jose, CA 95125";if(typeof(ebay)!="undefined")
ebay.oDocument.messageBox(str);else
alert(str);}}
if((lsiteid==0||lsiteid==1||lsiteid==100)&&oldstatevalue!=object.buyerstateprovince.value&&object.taxrate!=null)
calculatetotal(object.shippingfee,object.insurancefee);}

