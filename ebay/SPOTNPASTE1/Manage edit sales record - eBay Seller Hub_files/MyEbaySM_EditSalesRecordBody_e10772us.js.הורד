for(m=1;m<=numShippingMethods;m++){if((selfForm['ShippingMethod'+m])&&(selfForm['ShippingMethod'+m].value!="")){showItem('shippingservice'+m);showItem("ShippingServiceRemove"+m);if(selfForm['hdnSS'+m])selfForm['hdnSS'+m].value="1";}
else if((selfForm['ShippingMethod'+m])&&(selfForm['ShippingMethod'+m].value=="")){hideItem('shippingservice'+m);hideItem('ShippingServiceRemove'+m);if(selfForm['hdnSS'+m])selfForm['hdnSS'+m].value="0";break;}}
if(selfForm['ShippingMethod1'])showItem('shippingservice1');showHideAddLink(m-1,fId);updateShipInsurance(fId);if(selfForm.insurancefee)var ins='selfForm.insurancefee';else if(selfForm.insurance)var ins='selfForm.insurance';else if(selfForm.ShippingInsurance)var ins='selfForm.ShippingInsurance';if(ins&&typeof(calculatetotal)!='undefined')calculatetotal(selfForm['ShippingAndHandling1'],ins);}}
function AddShippingService(fId){selfForm=window.document.forms[fId];var showItemNum=1;for(m=1;m<=numShippingMethods;m++){if(ns4){if(document.layers['shippingservice'+m].visibility!='hide')showItemNum++;}
else if(ie4){if(document.all['shippingservice'+m].style.visibility!='none')showItemNum++;}
else if(ie5||ns6){if(document.getElementById('shippingservice'+m).style.display!='none')showItemNum++;}}
showHideAddLink(showItemNum,fId);for(m=1;m<=numShippingMethods;m++){if(ns4){if(document.layers['shippingservice'+m].visibility=='hide'){showItem('shippingservice'+m);break;}}
else if(ie4){if(document.all['shippingservice'+m].style.visibility=='none'){showItem('shippingservice'+m);break;}}
else if(ie5||ns6){if(document.getElementById('shippingservice'+m).style.display=='none'){showItem('shippingservice'+m);break;}}}}
function RemoveShipService(trId,fId){selfForm=window.document.forms[fId];hideItem('shippingservice'+trId);selfForm['hdnSS'+trId].value="0";selfForm['ShippingMethod'+trId].selectedIndex=0;if(selfForm['ShippingAndHandling'+trId])selfForm['ShippingAndHandling'+trId].value="";showItem('divAddAnother');hideItem('divShippingCalculator');}
function updateShippingService(nIndex,nSS){selfForm=window.document.forms[0];if(nIndex!=0)selfForm['hdnSS'+nSS].value="1";}
function updateShipInsurance(fId){selfForm=window.document.forms[fId];if((selfForm.insuranceoption)&&(selfForm.insuranceoption.type=='select-one')){var sel=selfForm.insuranceoption.selectedIndex;if(selfForm.insurancefee)var ins='selfForm.insurancefee';else if(selfForm.insurance)var ins='selfForm.insurance';else if(selfForm.ShippingInsurance)var ins='selfForm.ShippingInsurance';if(sel!=3)
shipInsuranceOnOff(selfForm.insuranceoption[0].selected,ins);else if(sel!=0)
shipInsuranceOnOff(selfForm.insuranceoption[3].selected,ins);}}
function shipInsuranceOnOff(on,field){if(document.all){if(on){eval(field+".disabled=true");return true;}
else{eval(field+".disabled=false");return false;}}
else{eval(field+".blur()");return true;}}
function SubmitForm(){for(m=2;m<=numShippingMethods;m++){if(selfForm['hdnSS'+m].value=="0"){selfForm['ShippingMethod'+m].value="-1";}}
selfForm.submit();}

