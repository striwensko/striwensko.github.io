function BuyerDetailsToggle(id,imgId,imgUrlDwn,imgUrlUp)
{var tableId=document.getElementById(id).style.display;if(tableId=="none"){document.getElementById(id).style.display="block";document.getElementById(imgId).src=imgUrlDwn;}
