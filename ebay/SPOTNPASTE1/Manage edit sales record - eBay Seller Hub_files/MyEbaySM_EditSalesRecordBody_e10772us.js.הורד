function calculatetotal(ShippingTXTBox,InsuranceTXTBox,SalesDiscountTXTBox,TaxChkBox){var tax,subtot,tot,insu,sh,TaxChkBox,discount,refundAmount,cod,CODFee;var u="undefined";cod=document.getElementById("cashondeliveryfee");if(lsiteid==0||lsiteid==1||lsiteid==100||lsiteid==2||lsiteid==203)
{var cntrl=document.forms['frmSubmit'].elements['taxincludeshippingflag'];if(typeof(cntrl)!=u)
TaxChkBox=cntrl.checked;var taxStateField=document.forms['frmSubmit'].elements['taxrate'];var taxState;if(typeof(taxStateField)!=u)
taxState=taxStateField.value;var buyerState=document.forms['frmSubmit'].elements['buyerstateprovince'];tax=taxState;if(typeof(tax)==u)
tax=0;}
else
{tax=0;}
var insBoxVal="",shipBoxVal="",salesDiscountBoxVal="",CODVal="";if(typeof(InsuranceTXTBox)!='undefined')
insBoxVal=InsuranceTXTBox.value;if(typeof(ShippingTXTBox)!='undefined')
shipBoxVal=ShippingTXTBox.value;if(cod)
CODVal=cod.value;if(typeof(SalesDiscountTXTBox)!='undefined'){var discountValue=SalesDiscountTXTBox.value;if(isNaN(discountValue)){var parseDiscountValue=parseValid(discountValue);parseDiscountValue=parseCountry(parseDiscountValue);if(parseDiscountValue==0){ebay.oDocument.messageBox(esrJSStrInvalidTextErr);parseDiscountValue="";salesDiscountBoxVal=parseDiscountValue;discount=convertToSummableCurrencyDiscount(ListedDecimal,GroupingSymbol,salesDiscountBoxVal);SalesDiscountTXTBox.value="";}else{salesDiscountBoxVal=parseDiscountValue;discount=convertToSummableCurrencyDiscount(ListedDecimal,GroupingSymbol,salesDiscountBoxVal);parseDiscountValue=formatAsMoney(discount);parseDiscountValue=parseDiscountValue.toString();parseDiscountValue=parseDiscountValue.replace(/\./g,ListedDecimal);SalesDiscountTXTBox.value=parseDiscountValue;}}else{salesDiscountBoxVal=discountValue;discount=convertToSummableCurrencyDiscount(ListedDecimal,GroupingSymbol,salesDiscountBoxVal);discountValue=formatAsMoney(discount);discountValue=discountValue.toString();discountValue=discountValue.replace(/\./g,ListedDecimal);SalesDiscountTXTBox.value=discountValue;}}
var refundAmountBoxVal;var RefundAmountTXTBox=document.forms['frmSubmit'].elements['refundamount'];if(typeof(RefundAmountTXTBox)==u){refundAmountBoxVal=0;}else{refundAmountBoxVal=RefundAmountTXTBox.value;}
refundAmount=convertToSummableCurrencyDiscount(ListedDecimal,GroupingSymbol,refundAmountBoxVal);subtot=convertToSummableCurrency(ListedDecimal,GroupingSymbol,subtotal);insu=convertToSummableCurrency(ListedDecimal,GroupingSymbol,insBoxVal);sh=convertToSummableCurrency(ListedDecimal,GroupingSymbol,shipBoxVal);if(cod)
CODFee=convertToSummableCurrency(ListedDecimal,GroupingSymbol,CODVal);var taxAmount;if(TaxChkBox)
taxAmount=(subtot+sh)*tax/100;else
taxAmount=(subtot)*tax/100;tot=(subtot+sh)+taxAmount;tot=tot-refundAmount;if(cod)
tot+=CODFee;var insOption=document.forms['frmSubmit'].insuranceoption;var insRequired=false;var insOptional=false;var isOptions=true;var INSURANCE_OPTIONAL=1;var INSURANCE_REQUIRED=2;var addOptionIns=(typeof(addInsuranceIfOptionalSelected)!=u&&addInsuranceIfOptionalSelected);if(typeof(insOption)!=u&&typeof(insOption.options)!=u)
{if(insOption.selectedIndex!=-1)
{insOptional=(insOption.options[insOption.selectedIndex].value==INSURANCE_OPTIONAL)?true:false;insRequired=(insOption.options[insOption.selectedIndex].value==INSURANCE_REQUIRED)?true:false;}}
else
isOptions=false;if(insRequired||(insOptional&&addOptionIns))
tot+=insu;if((discount<0.0)&&(Math.round(discount*-100)>Math.round((tot+refundAmount-subtot)*100))||((refundAmount!=0)&&(tot+discount)<0.0))
{if(typeof(SalesDiscountTXTBox)!='undefined')
SalesDiscountTXTBox.value="";if(tot<=0.0&&refundAmount!=0)
{var str=esrJSStrTotalSubtotalErr1;str+=esrJSPeriod;ebay.oDocument.messageBox(str);}else{var str=esrJSStrTotalSubtotalErr;if(esrJSCurrencyPrefix)str+=esrJSCurrency;if(refundAmount!=0)str+=formatAsMoney(tot);else str+=formatAsMoney(tot-subtot);if(!esrJSCurrencyPrefix)str+=esrJSCurrency;str+=esrJSPeriod;ebay.oDocument.messageBox(str);}}else{tot=tot+discount;}
if(lsiteid==0||lsiteid==1||lsiteid==100||lsiteid==2||lsiteid==203)
{taxAmount=formatAsMoney(taxAmount);if(isNaN(taxAmount))
taxAmount=0;taxAmount=taxAmount.toString();taxAmount=taxAmount.replace(/\./g,ListedDecimal);if(document.getElementById)
{var taxAmountField=document.getElementById("taxamountid");if(taxAmountField!=null)
taxAmountField.innerHTML=taxAmount;}
else if(document.all)
