{document.forms['frmSubmit'].elements['totalbox'].value=tot;}}
function convertToSummableCurrency(sListedDecimal,sGroupingSymbol,sPrice){if(typeof(sPrice)=='undefined'||sPrice.length==0)
return 0;var bOneListedDecimal=true;var bIsFreight=ebay.oDocument.oPage.oFreightConfig;for(var j=0;j<sPrice.length;j++){if(IsDigit(sPrice.charAt(j)))
{continue;}
else
{if(bIsFreight&&sPrice.charAt(j)==",")
{sPrice=sPrice.substring(0,j)+sPrice.substring(j+1);}
else if(sPrice.charAt(j)==ListedDecimal&&bOneListedDecimal)
{bOneListedDecimal=false;}
else
{return 0;}}}
if(sListedDecimal==".")
{sPrice=sPrice.replace(/\./g,'.');}
else
{sPrice=sPrice.replace(/\,/g,'.');}
sPrice=parseFloat(sPrice);if(isNaN(sPrice)||sPrice<0)
{return 0;}
else
{return sPrice;}}
function convertToSummableCurrencyDiscount(sListedDecimal,sGroupingSymbol,sPrice){if(sPrice.length==0)
return 0;var bOneListedDecimal=true;for(var j=0;j<sPrice.length;j++){if(IsDigit(sPrice.charAt(j))||sPrice.charAt(j)=='-')
{continue;}
else
{if(sPrice.charAt(j)==ListedDecimal&&bOneListedDecimal)
{bOneListedDecimal=false;}
else
{return 0;}}}
if(sListedDecimal==".")
{sPrice=sPrice.replace(/\./g,'.');}
else
{sPrice=sPrice.replace(/\,/g,'.');}
sPrice=parseFloat(sPrice);if(isNaN(sPrice))
{return 0;}
else
{return sPrice;}}
function IsDigit(cChar)
{if(cChar=="0"||cChar=="1"||cChar=="2"||cChar=="3"||cChar=="4"||cChar=="5"||cChar=="6"||cChar=="7"||cChar=="8"||cChar=="9")
{return true;}
else
{return false;}}
function formatAsMoney(mnt){mnt-=0;mnt=(Math.round(mnt*1000/10))/100;return(mnt==Math.floor(mnt))?mnt+'.00':((mnt*10==Math.floor(mnt*10))?mnt+'0':mnt);}
function insuranceChange(pThis,InsuFeeTextBox)
{if(pThis.selectedIndex==0||pThis.selectedIndex==3)
{InsuFeeTextBox.value="0";InsuFeeTextBox.disabled=true;InsuFeeTextBox.onfocus=InsuFeeTextBox.blur;}
else{InsuFeeTextBox.disabled=false;InsuFeeTextBox.onfocus=null;}}
function cashOnDeliveryChange(pThis,CashOnDeliveryTextBox)
{if(!pThis)
return;if(pThis.selectedIndex==0||pThis.selectedIndex==3)
{CashOnDeliveryTextBox.value="0";CashOnDeliveryTextBox.disabled=true;CashOnDeliveryTextBox.onfocus=CashOnDeliveryTextBox.blur;}
else{CashOnDeliveryTextBox.disabled=false;CashOnDeliveryTextBox.onfocus=null;}}
function taxChange(pThis,TaxTextBox)
{if(pThis)
{if(pThis.selectedIndex==0)
{TaxTextBox.value="0";TaxTextBox.onfocus=TaxTextBox.blur;}
else
{TaxTextBox.onfocus=null;}}}
